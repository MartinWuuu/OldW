<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.brave.oxygenerator">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/logo_1931"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".LoginActivity"
            android:label="登录">

        </activity>
        <activity
            android:name=".RegisterActivity"
            android:label="注册"
            android:parentActivityName=".LoginActivity" />
        <activity
            android:name=".MainActivity"
            android:label="@string/title_activity_main"
            android:parentActivityName=".LoginActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.brave.oxygenerator.LoginActivity" />
        </activity>
        <!--
 ATTENTION: This was auto-generated to add Google Play services to your project for
     App Indexing.  See https://g.co/AppIndexing/AndroidStudio for more information.
        -->
        <meta-data
            android:name="com.google.android.gms.version"
            android:value="@integer/google_play_services_version" />

        <activity
            android:name=".UserActivity"
            android:label="我的资料"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".PasswordActivity"
            android:label="@string/title_activity_password"
            android:parentActivityName=".MainActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.brave.oxygenerator.MainActivity" />
        </activity>
        <activity
            android:name=".AboutActivity"
            android:label="关于我们"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".AdviceActivity"
            android:label="意见反馈"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".ContactActivity"
            android:label="联系我们"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".UnlockActivity"
            android:label="解锁程序"
            />
        <activity
            android:name=".SetLockActivity"
            android:label="绘制锁屏图案"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".EditUserInfoActivity"
            android:parentActivityName=".UserActivity" />
        <activity
            android:name=".WelcomeActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>